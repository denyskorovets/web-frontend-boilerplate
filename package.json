{
  "name": "web-frontend-boilerplate",
  "version": "0.0.0-development",
  "description": "A boilerplate for developing frontend UI application",
  "keywords": [
    "boilerplate",
    "frontend",
    "web-frontend",
    "frontend-ui"
  ],
  "homepage": "https://github.com/dwiyatci/web-frontend-boilerplate",
  "bugs": "https://github.com/dwiyatci/web-frontend-boilerplate/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/web-frontend-boilerplate.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive --latest",
    "start": "webpack-dev-server",
    "webpack": "yarn clean && webpack",
    "build": "yarn clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf assets",
    "lint": "eslint --cache src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "bootstrap": "4.0.0-beta.2",
    "font-awesome": "4.7.0",
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "popper.js": "1.12.6",
    "tether": "1.4.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "bootstrap-loader": "2.2.0",
    "commitizen": "2.9.6",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "font-awesome-loader": "1.0.2",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "imports-loader": "0.7.1",
    "inline-manifest-webpack-plugin": "3.0.1",
    "json-loader": "0.5.7",
    "less": "2.7.3",
    "node-sass": "4.5.3",
    "postcss": "6.0.13",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.0.8",
    "resolve-url-loader": "2.1.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "semantic-release": "8.2.0",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
